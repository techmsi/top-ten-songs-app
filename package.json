{
  "name": "top-ten-songs-app",
  "version": "1.0.0",
  "description": "Find top 10 songs from spotify & add them to a custom playlist.",
  "main": "",
  "scripts": {
    "start": "npm build; npm run dev",
    "lint": "semistandard --verbose --fix | snazzy",
    "build": "webpack",
    "build:dev": "webpack -w",
    "build:css": "node-sass --output-style nested --include-path scss scss/app.scss public/css/app.css",
    "watch:css": "onchange 'scss/*.scss' -- npm run build:css",
    "dev": "lite-server"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "angular",
    "ng-redux",
    "spotify"
  ],
  "author": "techmsi",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "angular": "1.5.9",
    "angular-spotify": "^1.4.1",
    "http-server": "^0.9.0",
    "ng-redux": "^3.4.0-beta.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "html-loader": "^0.4.4",
    "lite-server": "^2.2.2",
    "node-sass": "^4.2.0",
    "nodemon": "^1.11.0",
    "onchange": "^3.2.1",
    "semistandard": "^11.0.0",
    "snazzy": "^7.0.0",
    "webpack": "^1.14.0"
  }
}
